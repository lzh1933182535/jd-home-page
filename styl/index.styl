vw_design = 750;
vw(px)
  px / vw_design * 100vw;
[class^="icon_"],
[class*=" icon_"] {
    background-image: url(../images/sprites.png);
    background-size: 200px 200px;
    background-repeat: no-repeat;
}
.flex
  display: flex;
  flex-wrap: wrap;
.br
  border-right: 1px solid #ddd;
.bb
  border-bottom: 1px solid #ddd;
.textColorRed
  color: #d8505c;
.oldPrice
  text-decoration: line-through;
.fz16
  font-size 16px;
.fz14
  font-size 14px;
.icon_logo
    position: absolute;
    left: 10px;
