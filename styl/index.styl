.jd-header
    position: fixed;
    left: 0;
    top: 0;
    width: 100%;
    height: vw(80);
    z-index: 1000;
    background-color rgba(201,21,35,0);
.jd-header-input
  width: vw(265*2);
  height: vw(60);
  margin-top: vw(10);
  margin-left: vw(140);
  padding-left: vw(60);
  border-radius: vw(30);
  border: none;
  outline: none;
  resize: none;
  -webkit-appearance: none;
.icon_search
    position: absolute;
    left: 80px;
    top: 10px;
    width: 20px;
    height: 20px;
    background-position: -60px -109px;
